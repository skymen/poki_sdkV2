          "IsEnabled": {
            "list-name": "SDK activado",
            "display-text": "El SDK está activado",
            "description": "Comprueba si el SDK está activado",
            "params": {}
          },
          "IsLoaded": {
            "list-name": "SDK cargado",
            "display-text": "El SDK está cargado",
            "description": "Comprueba si el SDK está cargado",
            "params": {}
          }
        },
        "expressions": {
          "LastAdTag": {
            "translated-name": "ÚltimaEtiquetaAnuncio",
            "description": "Última etiqueta de anuncio",
            "params": {}
          },
          "LastRewardedSuccess": {
            "translated-name": "ÚltimaRecompensaExitosa",
            "description": "Última recompensa exitosa",
            "params": {}
          }
        }
      }
    }
  }
