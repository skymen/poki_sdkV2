{
  "languageTag": "es-ES",
  "fileDescription": "Cadenas para skymen_pokisdk.",
  "text": {
    "plugins": {
      "skymen_pokisdk": {
        "name": "Poki SDK",
        "description": "Plugin sencillo para juegos alojados en la plataforma Poki (https://sdk.poki.com/)",
        "help-url": "https://sdk.poki.com/",
        "aceCategories": {
          "ads": "anuncios",
          "analytics": "analítica",
          "gameplay": "jugabilidad",
          "misc": "varios"
        },
        "properties": {
          "enabled": {
            "name": "Activado",
            "desc": "Indica si el SDK está activado",
            "items": {
              "disabled": "Desactivado",
              "enabled_export": "Activado al exportar",
              "enabled": "Activado"
            }
          },
          "debug": {
            "name": "Depurar en vista previa",
            "desc": "Indica si se activa el modo depuración"
          },
          "gameId": {
            "name": "ID del juego",
            "desc": "Introduce el ID del juego aquí para que funcione el beacon"
          },
          "doBeacon": {
            "name": "Enviar beacon",
            "desc": "Indica si se envían beacons a Poki"
          },
          "beaconInterval": {
            "name": "Intervalo de beacon",
            "desc": "Número de segundos entre beacons"
          },
          "maxBeacons": {
            "name": "Máximo de beacons",
            "desc": "Número máximo de beacons a enviar"
          },
          "loadingNotification": {
            "name": "Notificación de carga",
            "desc": "Cuándo notificar al DOM que el juego ha terminado de cargar",
            "items": {
              "immediate": "Inmediato",
              "afterFirstLayout": "Después del primer diseño",
              "manual": "Manual"
            }
          },
          "automaticSuspend": {
            "name": "Suspensión automática",
            "desc": "Suspende automáticamente el SDK cuando el juego se suspende"
          },
          "suspendTimeout": {
            "name": "Tiempo de espera para suspender",
            "desc": "Número de segundos antes de suspender el SDK"
          },
          "autoDelayGameplayStart": {
            "name": "Retraso automático del inicio de juego",
            "desc": "Retrasa automáticamente el inicio del juego para esperar la interacción del usuario"
          }
        },
        "actions": {
          "Interstitial": {
            "list-name": "Intersticial",
            "display-text": "Solicitar anuncio intersticial [i]{0}[/i]",
            "description": "Solicita un anuncio intersticial",
            "params": {
              "tag": {
                "name": "Etiqueta",
                "desc": "La etiqueta del anuncio"
              }
            }
          },
          "Rewarded": {
            "list-name": "Recompensado",
            "display-text": "Solicitar anuncio recompensado [i]{0}[/i]",
            "description": "Solicita un anuncio recompensado",
            "params": {
              "tag": {
                "name": "Etiqueta",
                "desc": "La etiqueta del anuncio"
              }
            }
          },
          "SendEvent": {
            "list-name": "Enviar evento",
            "display-text": "Enviar evento de analítica [i]{0}[/i], [i]{1}[/i], [i]{2}[/i]",
            "description": "Envía un evento de analítica",
            "params": {
              "category": {
                "name": "Categoría",
                "desc": "La categoría del evento"
              },
              "action": {
                "name": "Qué",
                "desc": "Sobre qué trata el evento"
              },
              "label": {
                "name": "Acción",
                "desc": "La acción realizada"
              }
            }
          },
          "GameplayStart": {
            "list-name": "Inicio de juego",
            "display-text": "Inicio de juego",
            "description": "Notifica al SDK que el juego ha comenzado",
            "params": {}
          },
          "GameplayStop": {
            "list-name": "Fin de juego",
            "display-text": "Fin de juego",
            "description": "Notifica al SDK que el juego ha terminado",
            "params": {}
          },
          "LoadingEnd": {
            "list-name": "Fin de carga",
            "display-text": "Fin de carga",
            "description": "Notifica manualmente al SDK que el juego ha terminado de cargar",
            "params": {}
          },
          "MovePill": {
            "list-name": "Mover Píldora",
            "display-text": "Mover píldora [i]{0}[/i]% desde arriba + [i]{1}[/i]px",
            "description": "Reposicionar la Píldora Poki en móvil. topPercent (0-50) establece la posición vertical como porcentaje desde arriba, topPx es el desplazamiento adicional en píxeles (positivo mueve hacia abajo, negativo hacia arriba)",
            "params": {
              "topPercent": {
                "name": "Porcentaje Superior",
                "desc": "Posición vertical como porcentaje desde arriba (0-50)"
              },
              "topPx": {
                "name": "Píxeles Superiores",
                "desc": "Desplazamiento adicional en píxeles (positivo mueve hacia abajo, negativo hacia arriba)"
              }
            }
          }
        },
        "conditions": {
          "LastRewardedSuccess": {
            "list-name": "Última recompensa exitosa",
            "display-text": "Última recompensa exitosa",
            "description": "Última recompensa exitosa",
            "params": {}
